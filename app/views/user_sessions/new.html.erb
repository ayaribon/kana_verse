<div class="main">
  <div class="form_box">
    <%= form_with url: login_path do |form| %>
      <div class="form_title">ログイン</div>

      <%= form.label :email, "メールアドレス" %>
      <%= form.email_field :email, class: "form_user" %>
  
      <%= form.label :password, "パスワード" %>
      <%= form.password_field :password, class: "form_user" %>

      <%= form.submit "ログイン", class: "form_user submit" %>
    <% end %>
  </div>
</div>